{
  "name": "timsp430",
  "title": "TI MSP430",
  "description": "MSP430 microcontrollers (MCUs) from Texas Instruments (TI) are 16-bit, RISC-based, mixed-signal processors designed for ultra-low power. These MCUs offer the lowest power consumption and the perfect mix of integrated peripherals for thousands of applications.",
  "url": "http://www.ti.com/lsds/ti/microcontrollers_16-bit_32-bit/msp/overview.page",
  "homepage": "http://platformio.org/platforms/timsp430",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-timsp430.git"
  },
  "version": "2.0.4",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-arduinomsp430",
      "script": "builder/frameworks/arduino.py"
    },
    "energia": {
      "package": "framework-energiamsp430",
      "script": "builder/frameworks/energia.py"
    }
  },
  "packages": {
    "toolchain-timsp430": {
      "type": "toolchain",
      "version": "~1.40603.0"
    },
    "framework-arduinomsp430": {
      "type": "framework",
      "optional": true,
      "version": "~1.10601.0"
    },
    "framework-energiamsp430": {
      "type": "framework",
      "optional": true,
      "version": "1.10610.0"
    },
    "tool-mspdebug": {
      "type": "uploader",
      "optional": true,
      "version": "~1.2200.0"
    }
  }
}
